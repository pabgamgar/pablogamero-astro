---
interface Props {
  navLinks: {
    label: string;
    href: string;
  }[];
}

const { navLinks } = Astro.props;
---

<div class="container">
  {navLinks.map((link) => <a href={link.href}>{link.label}</a>)}
</div>

<style>
  .container {
    display: flex;
    justify-content: space-around;
    align-items: center;
    padding: 1rem 0;
    position: absolute;
    padding: 1rem;
    background-color: aqua;
  }
  a {
    text-decoration: none;
    color: var(--primary);
    font-size: 1.5rem;
  }
</style>
